{
  "name": "malanghub-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "rm -rf node_modules && rm -rf apps/**/node_modules && rm -rf packages/**/node_modules && rm -rf dist && rm -rf apps/**/dist && rm -rf packages/**/dist && rm -rf .turbo && rm -rf pnpm-lock.yaml",
    "reset": "pnpm run clean && pnpm install"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "volta": {
    "node": "24.11.0",
    "pnpm": "10.20.0"
  }
}
