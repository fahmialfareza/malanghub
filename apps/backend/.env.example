
# MongoDB connection URI (example)
MONGO_URI=mongodb://localhost:27017
PORT=8080


# Authentication
# A long, random string. Keep secret in production.
JWT_SECRET=changeme
# Optional: JWT token TTL (example: 24h, 7d)
JWT_EXPIRES_IN=24h

# Google OAuth (optional)
# If you enable Google Sign-In, set these to your OAuth client credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8080/api/auth/google/callback

# Redis (optional)
REDIS_ADDR=localhost:6379
REDIS_DB=0
REDIS_PASSWORD=

# New Relic (optional)
# Set NEW_RELIC_ENABLED=false to disable even if keys exist
NEW_RELIC_ENABLED=true
NEW_RELIC_APP_NAME=MyApp
NEW_RELIC_LICENSE_KEY=
# Set to true to enable debug logging from the agent
NEW_RELIC_DEBUG=false

# Misc
# GIN_MODE can be "debug", "release", or "test"
GIN_MODE=debug

# Cloudinary (optional)
# Provide either `CLOUDINARY_URL` or the individual pieces below.
# Example URL: cloudinary://API_KEY:API_SECRET@CLOUD_NAME
CLOUDINARY_URL=
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
